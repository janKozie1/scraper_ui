{

  "manifest_version": 2,
  "name": "scraper_ui",
  "version": "1.0",
  "description": "For generating web scraping config files",
  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*"],
      "js": ["index.js"]
    }
  ]

}
